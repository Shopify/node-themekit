{
  "name": "node-themekit",
  "version": "0.4.1",
  "description": "Node version of ThemeKit",
  "main": "index.js",
  "license": "MIT",
  "author": "Shopify Inc.",
  "dependencies": {
    "async": "2.0.1",
    "bin-wrapper": "3.0.2",
    "bluebird": "3.4.6"
  },
  "devDependencies": {
    "eslint": "3.3.1",
    "eslint-plugin-shopify": "14.0.0",
    "mocha": "3.0.2"
  },
  "scripts": {
    "test": "node test/themekit-version",
    "test-mocha": "npm run lint-allow-warning && mocha -u tdd test/index.js",
